{
    "name": "slavkluev/yii2-ip2location",
    "type": "yii2-extension",
    "description": "A Yii2 extension allows you to get the user's location by IP address. Use a free Ip2Location LITE database. Auto-update database.",
    "keywords": [
        "slavkluev",
        "yii2-ip2location",
        "ip2location",
        "yii2",
        "location",
        "geo",
        "geolocation"
    ],
    "homepage": "https://github.com/slavkluev/yii2-ip2location",
    "license": "MIT",
    "authors": [
        {
            "name": "Viacheslav Kliuev",
            "email": "slavkluev@yandex.ru",
            "homepage": "https://github.com/slavkluev",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.4",
        "ext-zip": "*",
        "guzzlehttp/guzzle": "^7.8",
        "ip2location/ip2location-php": "^9.7"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "slavkluev\\Ip2Location\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "slavkluev\\Ip2Location\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "bootstrap": "slavkluev\\Ip2Location\\Bootstrap",
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
